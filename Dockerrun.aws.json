{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "georgeobr/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "georgeobr/multi-server",
      "hostname": "server",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "georgeobr/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128

    },
    {
      "name": "nginx",
      "image": "georgeobr/multi-nginx",
      "essential": false,
      "memory": 128,
      "portMapping": [
        {"hostPort": 80,
        "containerPort": 80
        }
      ],
      "links": ["client", "server", "worker"]

    }

  ]
}