{
  "AWSEBDockerrunVersion": 2,
  "containerDefiitions": [
    {
      "name": "client",
      "image": "georgeobr/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "georgeobr/multi-server",
      "hostname": "server",
      "essential": false
    },
    {
      "name": "worker",
      "image": "georgeobr/multi-worker",
      "hostname": "worker",
      "essential": false

    },
    {
      "name": "nginx",
      "image": "georgeobr/multi-nginx",
      "essential": true,
      "portMapping": [
        {"hostPort": 80,
        "containerPort": 80
        }
      ],
      "links": ["client", "server"]

    }


  ]






}